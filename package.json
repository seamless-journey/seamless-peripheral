  },
  "homepage": "https://github.com/seamless-journey/seamless-peripheral#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "jest": "^20.0.4",
    "np": "^2.16.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "eddystone-beacon": "^1.0.7",
    "google-url": "0.0.4",
    "predator-quotes": "^1.1.16",
